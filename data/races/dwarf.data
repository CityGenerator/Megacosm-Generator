LPUSH npc_race dwarf

SET dwarf_details      {"name": "Dwarf",      "size": "medium",  "description": "their stubby limbs and strict work ethic"}

LPUSH dwarf_covering skin

SET dwarf_subrace_chance 30
# Note that descriptions aren't heavily used right now anyways.
LPUSH dwarf_subrace mountain
LPUSH dwarf_subrace hill
LPUSH dwarf_subrace deep
LPUSH dwarf_subrace gray
LPUSH dwarf_subrace desert
LPUSH dwarf_subrace arctic
LPUSH dwarf_subrace wild
LPUSH dwarf_subrace degenerate

HSET dwarf_subrace_description mountain   {"subrace": "Mountain Dwarf",   "description": "" }
